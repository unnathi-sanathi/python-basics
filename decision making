#Greatest of two numbers

a=int(input())
b=int(input())
if(a>b):
    print(a,'is greater than', b)
elif(a<b):
     print(a,'is less than', b)
else:
    print(a ,'is equal to', b)
#Vowel Or Consonants
a=input()
if a in "aeiouAEIOU":
    print('vowel')
elif a in "bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ":
    print('consonant')
else:
    print('Not an alphabet')
#Grading System
a=int(input())
if(a==100):
    print('S')
elif(a>=90 and a<100):
    print('A')
elif(a>=80 and a<90):
    print('B')
elif(a>=70 and a<80):
    print('C')
elif(a>=60 and a<70):
    print('D')
elif(a>=50 and a<60):
    print('E')
elif(a<50):
    print('F')
else:
    print('Invalid input')
#Profit or Loss
cp=float(input())
soo=float(input())
sp=12*soo
print('Enter the price of a dozen mangoes')
print('Enter the price at which 1 mango is being sold')
if(sp>cp):
    print("profit :\t Rs %.2f"%(sp-cp))
elif(sp<cp):
    print("loss :\t Rs %.2f"%(cp-sp))
else:
    print("no profit nor loss")
#Fee Collection
print("Enter the student type")
s=input()
s=s.upper()
if s=="MSDS":
    print("Enter tuition fee")
    t=float(input())
    print("Enter bus fee")
    b=float(input())
    a=t+b
    print("The fees to be paid by the student is Rs.%.2f"%a)
elif s=="MSH":
    print("Enter tuition fee")
    t=float(input())
    print("Enter hostel fee")
    h=float(input())
    x=t+h
    print("The fees to be paid by the student is Rs.%.2f"%x)
elif s=="MGSDS":
    print("Enter tuition fee")
    t=float(input())
    t=1.5*t
    print("Enter bus fee")
    b=float(input())
    d=t+b
    print("The fees to be paid by the student is Rs.%.2f"%d)
elif s=="MGSH":
    print("Enter tuition fee")
    t=float(input())
    t=t*1.5
    print("Enter hostel fee")
    h=float(input())
    c=t+h
    print("The fees to be paid by the student is Rs.%.2f"%c)
